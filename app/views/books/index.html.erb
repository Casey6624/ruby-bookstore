<div class="container">
<h2> Books Available </h2>
<table class="table">
<tr>
<th>Book Id</th>
<th>Book Title</th>
<th>Book Price</th>
<th>Book ID</th>
<th>Publisher</th>
<th>Description</th>
</tr>
<tr>
<% @books.each do |book| %>
<td> <%= book.id %> </td>
<td> <%= book.title %> </td>
<td> <%= book.price %> </td>
<td> <%= book.book_id %> </td>
<td> <%= book.publisher %> </td>
<td> <%= book.description %> </td>
</tr>
<% end %>
</table>
</div>